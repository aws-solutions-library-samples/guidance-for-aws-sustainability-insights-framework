{
	"name": "@sif/integration-tests",
	"description": "SIF: Integration Tests.",
	"version": "3.16.0",
	"author": "Dean Hart <deanhart@amazon.com>",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsc -b && npx shx cp 'src/features/*/*.feature' 'dist/features/'",
		"generate:token": "rushx build && node dist/features/support/generateToken.js",
		"generate:postman:environment": "rushx build && node dist/features/support/generatePostmanEnvironment.js",
		"generate:insomnia:environment": "rushx build && node dist/features/support/generateInsomniaEnvironment.js",
		"seed": "rushx build && node dist/features/support/seedData.js",
		"test": "npm run build && npx cucumber-js",
		"test:integration": "npm run build && npx cucumber-js dist/features/* --tags \"not @loadTest\"",
		"test:load": "npm run build && npx cucumber-js dist/features/load_test* --tags @loadTest",
		"test:parallel": "npm run build && npx cucumber-js dist/features/* --tags @accessManagement && concurrently \"npx cucumber-js dist/features/* --tags @calculations\" \"npx cucumber-js dist/features/* --tags @impacts\" \"npx cucumber-js dist/features/* --tags @referenceDataSets\"  \"npx cucumber-js dist/features/* --tags @pipelines\" \"npx cucumber-js dist/features/* --tags @pipelineProcessor\" \"npx cucumber-js dist/features/* --tags @endToEnd\""

	},
	"dependencies": {
		"@aws-sdk/client-cognito-identity-provider": "3.370.0",
		"@aws-sdk/client-dynamodb": "3.370.0",
		"@aws-sdk/client-lambda": "3.370.0",
		"@aws-sdk/client-sfn": "3.370.0",
		"@aws-sdk/client-ssm": "3.370.0",
		"@aws-sdk/lib-dynamodb": "3.370.0",
		"@aws-sdk/client-s3": "3.370.0",
		"@aws-sdk/client-cloudformation": "3.370.0",
		"@aws-sdk/client-kinesis": "3.370.0",
		"@cucumber/cucumber": "9.3.0",
		"@sif/lambda-invoker": "workspace:*",
		"axios": "1.4.0",
		"apickli": "3.0.2",
		"form-data": "4.0.0",
		"fast-glob": "3.3.0",
		"jwt-encode": "1.0.1",
		"pino": "8.14.1",
		"@aws-amplify/auth": "5.5.4",
		"jsonpath-plus": "7.2.0",
		"convert-csv-to-array": "1.0.3",
		"ulid" :"2.3.0"
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "0.2.2",
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@rushstack/eslint-config": "3.3.2",
		"@types/jwt-encode": "1.0.0",
		"@types/node": "20.4.2",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"eslint": "8.45.0",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"concurrently": "8.2.0"
	},
	"private": false
}
