{
	"name": "@sif/integration-tests",
	"description": "SIF: Integration Tests.",
	"version": "3.12.0",
	"author": "Dean Hart <deanhart@amazon.com>",
	"scripts": {
		"clean": "npx shx rm -rf dist *.log",
		"build": "npx tsc -b && npx shx cp 'src/features/*/*.feature' 'dist/features/'",
		"generate:token": "rushx build && node dist/features/support/generateToken.js",
		"generate:postman:environment": "rushx build && node dist/features/support/generatePostmanEnvironment.js",
		"seed": "rushx build && node dist/features/support/seedData.js",
		"test": "npm run build && npx cucumber-js",
		"test:integration": "npm run build && npx cucumber-js dist/features/"
	},
	"dependencies": {
		"@aws-sdk/client-cognito-identity-provider": "3.229.0",
		"@aws-sdk/client-dynamodb": "3.229.0",
		"@aws-sdk/client-lambda": "3.229.0",
		"@aws-sdk/client-ssm": "3.229.0",
		"@aws-sdk/lib-dynamodb": "3.229.0",
		"@cucumber/cucumber": "8.5.2",
		"@sif/lambda-invoker": "workspace:*",
		"axios": "1.1.2",
		"apickli": "3.0.2",
		"form-data": "4.0.0",
		"fast-glob": "3.2.12",
		"jwt-encode": "1.0.1",
		"pino": "8.6.1",
		"@aws-amplify/auth": "4.6.8"
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "0.1.1",
		"@esbuild-plugins/node-modules-polyfill": "0.1.4",
		"@rushstack/eslint-config": "3.0.0",
		"@tsconfig/node16": "1.0.3",
		"@types/jwt-encode": "1.0.0",
		"@types/node": "16.11.7",
		"@types/prettier": "2.6.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"eslint": "8.24.0",
		"shx": "0.3.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4"
	},
	"private": false
}
