{
	"name": "@sif/concurrency-manager",
	"description": "SIF: Concurrency Manager.",
	"version": "0.2.0",
	"author": "William Sia <willsia@amazon.com>",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsc -b",
		"test": "npm run build && vitest run --passWithNoTests",
		"build:bundled": "npx ts-node --esm --prefer-ts-exts esbuild.ts",
		"start:bundled": "node dist/app.mjs"
	},
	"dependencies": {
		"aws-lambda": "1.0.7",
		"pino": "8.6.1",
		"@sif/dynamodb-utils": "workspace:*",
		"@sif/validators": "workspace:*"
	},
	"devDependencies": {
		"@aws-sdk/types": "3.226.0",
		"@aws-sdk/client-sqs": "3.229.0",
		"@aws-sdk/client-sfn": "3.229.0",
		"@aws-sdk/client-lambda": "3.229.0",
		"@aws-sdk/client-dynamodb": "3.229.0",
		"@aws-sdk/client-eventbridge": "3.229.0",
		"@aws-sdk/lib-dynamodb": "3.229.0",
		"@esbuild-plugins/node-globals-polyfill": "0.1.1",
		"@esbuild-plugins/node-modules-polyfill": "0.1.4",
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/aws-lambda": "8.10.106",
		"@types/node": "18.16.19",
		"@types/prettier": "2.7.1",
		"@typescript-eslint/eslint-plugin": "5.38.1",
		"@typescript-eslint/parser": "5.38.1",
		"dotenv": "16.0.3",
		"esbuild": "0.15.10",
		"eslint": "8.24.0",
		"pino-pretty": "9.1.0",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "4.8.4",
		"vitest": "0.23.4",
		"vitest-mock-extended": "0.1.16"
	},
	"private": false
}
