		"dotenv": "16.3.1",
		"esbuild": "0.18.14",
		"eslint": "8.45.0",
		"pino": "8.14.1",
		"pino-pretty": "10.0.1",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"vitest": "0.33.0",
		"vitest-mock-extended": "1.1.4"
	},
