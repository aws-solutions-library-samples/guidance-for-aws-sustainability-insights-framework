{
	"name": "@sif/schema-migrator",
	"description": "SIF: Schema Migrator.",
	"version": "0.2.0",
	"author": "William Sia <willsia@amazon.com>",
	"type": "module",
	"scripts": {
		"clean": "npx shx rm -rf dist *.log",
