{
	"name": "@sif/proxy",
	"version": "1.2.0",
	"description": "Sustainability SaaS: Common proxy related functionality",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
