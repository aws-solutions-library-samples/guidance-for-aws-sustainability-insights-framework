		"fastify": "4.12.0",
		"@aws-sdk/client-lambda": "3.224.0",
		"jwt-decode": "~3.1.2",
		"pino": "8.6.1",
		"jwt-encode": "1.0.1",
		"@types/jwt-encode": "1.0.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "18.16.19",
		"@types/prettier": "2.6.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"eslint": "8.24.0",
		"shx": "0.3.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4",
		"pino": "8.6.1",
		"vitest": "0.23.4",
		"vitest-mock-extended": "0.1.16"
