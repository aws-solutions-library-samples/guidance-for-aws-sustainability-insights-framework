{
	"name": "@sif/dynamodb-utils",
	"version": "2.1.0",
	"description": "Common DynamoDB utils.",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "tsc -b",
		"test": "npm run build && vitest run --passWithNoTests"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "3.370.0",
		"@aws-sdk/lib-dynamodb": "3.370.0",
		"@aws-sdk/smithy-client": "3.370.0",
		"@aws-sdk/types": "3.370.0",
		"@aws-sdk/util-dynamodb": "3.370.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "20.4.2",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"eslint": "8.45.0",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"aws-sdk-client-mock": "3.0.0",
		"vitest": "0.33.0",
		"vitest-mock-extended": "1.1.4",
		"pino": "8.14.1",
		"pino-pretty": "10.0.1"
	},
	"private": false
}
