	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "16.11.7",
		"@types/prettier": "2.6.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"eslint": "8.24.0",
		"shx": "0.3.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4",
		"aws-sdk-client-mock": "2.0.0",
		"vitest": "0.23.4",
		"vitest-mock-extended": "0.1.16",
		"pino": "8.6.1",
		"pino-pretty": "9.1.1"
