{
	"name": "@sif/lambda-invoker",
	"version": "1.1.0",
	"description": "Lambda Invoker",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"main": "./dist/cjs/index.cjs",
	"types": "./dist/esm/index.d.ts",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build:tsc": "npx tsc --project tsconfig.json",
		"build": "node --no-warnings=ExperimentalWarning --loader ts-node/esm esbuild.ts",
		"test": "npm run build && vitest run"
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "3.370.0",
		"@aws-sdk/util-utf8-node": "3.259.0",
		"http-errors": "2.0.0",
		"js-yaml": "4.1.0",
		"readable-stream": "4.4.2"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/antlr4-autosuggest": "0.0.1",
		"@types/antlr4": "4.11.2",
		"@types/http-errors": "2.0.1",
		"@types/node": "20.11.20",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"antlr4ts-cli": "0.5.0-alpha.4",
		"aws-sdk-client-mock": "3.0.0",
		"esbuild": "0.18.14",
		"eslint": "8.45.0",
		"pino": "8.14.1",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"vitest": "0.33.0",
		"vitest-mock-extended": "1.1.4"
	},
	"private": false
}
