{
	"name": "@sif/lambda-invoker",
	"version": "1.1.0",
	"description": "Lambda Invoker",
	"author": "Dean Hart <deanhart@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"main": "./dist/cjs/index.cjs",
	"types": "./dist/esm/index.d.ts",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build:tsc": "npx tsc --project tsconfig.json",
		"build": "npx ts-node --esm --prefer-ts-exts esbuild.ts",
		"test": "npm run build && vitest run"
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "3.229.0",
		"@aws-sdk/util-utf8-node": "3.208.0",
		"http-errors": "1.8.0",
		"js-yaml": "4.1.0",
		"readable-stream": "3.6.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/antlr4-autosuggest": "0.0.0",
		"@types/antlr4": "4.7.2",
		"@types/http-errors": "1.8.2",
		"@types/node": "18.16.19",
		"@types/prettier": "2.6.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"antlr4ts-cli": "0.5.0-alpha.4",
		"aws-sdk-client-mock": "2.0.0",
		"esbuild": "0.15.9",
		"eslint": "8.24.0",
		"pino": "8.6.1",
		"shx": "0.3.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4",
		"vitest": "0.23.4",
		"vitest-mock-extended": "0.1.16"
	},
	"private": false
}
