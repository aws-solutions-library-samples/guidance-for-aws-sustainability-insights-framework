{
	"name": "@sif/cdk-common",
	"version": "1.0.0",
	"description": "CDK constructs and shared utilities",
	"author": "Edmund Chute <echute@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "tsc -b && npm run bundle:lambda",
		"bundle:lambda": "rm -rf dist/dynamodb-seeder/lambdas && npx ts-node --esm --prefer-ts-exts esbuild.ts",
		"test": "npm run build && vitest run --passWithNoTests"
	},
	"dependencies": {
		"aws-cdk-lib": "2.87.0",
		"constructs": "10.2.69",
		"aws-lambda": "1.0.7",
		"@aws-sdk/client-dynamodb": "3.370.0",
		"@aws-sdk/client-s3": "3.370.0",
		"@aws-sdk/lib-dynamodb": "3.370.0"
	},
	"devDependencies": {
		"cdk-nag": "2.27.73",
		"@types/aws-lambda": "8.10.119",
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "20.4.2",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"eslint": "8.45.0",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"esbuild": "0.18.14",
		"aws-cdk": "2.87.0",
		"vitest": "0.33.0",
		"vitest-mock-extended": "1.1.4",
		"@types/lodash.chunk": "4.2.7",
		"custom-resource-helper": "1.0.15",
		"lodash.chunk": "4.2.0"
	},
	"private": false
}
