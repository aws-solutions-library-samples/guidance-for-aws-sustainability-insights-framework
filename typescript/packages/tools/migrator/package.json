{
	"name": "@sif/migrator",
	"description": "SIF: Tools and utilities related to migrations",
	"version": "0.0.1",
	"author": "Hassan khokhar <hkhokhar@amazon.com>",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.js",
	"type": "module",
	"scripts": {
		"clean": "npx shx rm -rf dist *.log tsconfig.tsbuildinfo",
		"build": "npx tsc -b",
		"migrate:metrics": "rushx build && node dist/app.js",
		"test": "npm run build && vitest run --passWithNoTests"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "3.229.0",
		"@aws-sdk/util-dynamodb": "3.229.0",
		"@aws-sdk/smithy-client": "3.226.0",
		"@aws-sdk/types": "3.226.0",
		"@aws-sdk/client-ssm": "3.299.0",
		"@aws-sdk/client-s3": "3.299.0",
		"@aws-sdk/client-sqs": "3.299.0",
		"@sif/dynamodb-utils": "workspace:*",
		"clipanion": "3.2.0",
		"dayjs": "1.11.7",
		"ulid": "2.3.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "16.11.7",
		"@types/prettier": "2.6.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"@types/pg": "8.6.6",
		"eslint": "8.24.0",
		"pino": "8.6.1",
		"shx": "0.3.3",
		"ts-node": "10.9.1",
		"typescript": "4.8.4",
		"vitest": "0.23.4",
		"vitest-mock-extended": "0.1.16"
	},
	"private": false
}
