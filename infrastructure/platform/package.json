{
	"name": "@sif/infrastructure-platform",
	"version": "2.7.1",
	"description": "SIF: Common Platform Infrastructure.",
	"author": "Dean Hart <deanhart@amazon.com>",
	"type": "module",
	"scripts": {
		"clean:package": "npx shx rm -rf dist *.log .rush tsconfig.tsbuildinfo",
		"build": "npx tsc -b && npm run build:validator",
		"build:validator": "node_modules/.bin/ts-node --esm --prefer-ts-exts esbuild.ts",
		"test": "npm run build && vitest run --passWithNoTests",
		"predeploy": "node_modules/.bin/ts-node --esm --prefer-ts-exts src/predeploy.ts",
		"cdk": "npm run predeploy && cdk"
	},
	"dependencies": {
		"aws-cdk-lib": "2.87.0",
		"constructs": "10.2.69",
		"@aws-sdk/client-ec2": "3.370.0",
		"@aws-sdk/client-iam": "3.370.0",
		"@aws-sdk/client-acm": "3.370.0",
		"@aws-sdk/client-secrets-manager": "3.370.0",
		"@aws-sdk/client-rds": "3.370.0",
		"source-map-support": "0.5.21",
		"pino": "8.14.1",
		"pino-pretty": "10.0.1",
		"cdk-nag": "2.27.73",
		"@sif/cdk-common": "workspace:*",
		"@sif/dynamodb-utils": "workspace:*",
		"@aws-cdk/aws-sagemaker-alpha": "2.87.0-alpha.0",
		"shelljs": "0.8.5",
		"@inquirer/prompts": "3.0.3",
		"@inquirer/select": "1.2.8",
		"@inquirer/confirm": "2.0.9",
		"ajv": "8.12.0"
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "0.2.2",
		"@tsconfig/node16-strictest-esm": "1.0.3",
		"@types/node": "20.4.2",
		"@types/prettier": "2.7.3",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"aws-cdk": "2.87.0",
		"esbuild": "0.18.14",
		"eslint": "8.45.0",
		"pnpm": "8.6.8",
		"shx": "0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"vitest": "0.33.0",
		"vitest-mock-extended": "1.1.4"
	},
	"private": false
}
